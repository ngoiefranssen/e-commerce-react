{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "prettier/prettier": ["error", { "singleQuote": true, "parser": "flow" }],
  "rules": {
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}
